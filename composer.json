{
    "name": "taking/flarum-ext-korean",
    "type": "flarum-extension",
    "description": "Korean language pack.",
    "keywords": ["locale","flarum"],
    "license": "MIT",
    "authors": [
    {
        "name": "taking",
        "email": "ceo@taking.kr"
    }
    ],
    "support": {
      "issues": "https://github.com/taking/flarum-ext-korean/issues",
      "source": "https://github.com/taking/flarum-ext-korean"
    },
    "require": {
        "flarum/core": "^1.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Korean",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "kr",
            "title": "Korean"
        }
    }
}
